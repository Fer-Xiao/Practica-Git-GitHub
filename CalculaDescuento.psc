Algoritmo CalculaDescuento
	Definir importeTotal Como Real;
	Definir tieneDescuento Como Caracter;
	Escribir "Introduce el importe total";
	Leer importeTotal;
	Escribir "Tienes descuento (Si/No)";
	Leer tieneDescuento;
	
	si (tieneDescuento = "si") o (tieneDescuento = "Si") o (tieneDescuento = "SI")
		Escribir "LALALALALALA";
	SiNo
		
	FinSi
FinAlgoritmo
